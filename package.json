{
  "name": "geonorm",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "PORT=${PORT:-5174} vite --config vite.config.ts --host 0.0.0.0 --port $PORT",
    "dev:vercel": "PORT=${PORT:-5174} vite --host 0.0.0.0 --port ${PORT:-5174}",
    "build": "vite build",
    "lint": "eslint .",
    "lint:check": "npm run lint && npx tsc --noEmit",
    "preview": "vite preview",
    "server": "tsx backend/index.ts",
    "server:dev": "nodemon --exec tsx backend/index.ts",
    "vercel:dev": "vercel dev",
    "vercel:prod": "vercel --prod",
    "start:all": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "start:vercel": "concurrently \"npm run vercel:dev\" \"npm run dev\"",
    "start:hybrid": "concurrently \"npm run server:dev\" \"npm run vercel:dev\" \"npm run dev\"",
    "start:production": "npm run vercel:prod"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@turf/turf": "^7.2.0",
    "@types/nodemailer": "^7.0.1",
    "@types/shapefile": "^0.6.4",
    "@vercel/node": "^5.3.24",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "lucide-react": "^0.542.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.6",
    "proj4": "^2.19.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shapefile": "^0.6.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "twilio": "^5.9.0",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": ">=20 <23"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^1.4.12",
    "@types/node": "20.14.10",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vercel": "^48.1.6",
    "vite": "^5.4.20"
  },
  "overrides": {
    "@types/node": "20.14.10"
  }
}
